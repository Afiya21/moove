{{!
    @template theme_moove/header

    Page header template

    Example context (json): {}
}}

{{> theme_moove/accessibilitybar }}
<header class="animate">
<nav class="fixed-top navbar navbar-light navbar-expand moodle-has-zindex">
    {{#hasdrawertoggle}}
        <div data-region="drawer-toggle" class="d-inline-block mr-3 drawer-toggle">
            <button aria-expanded="{{#navdraweropen}}true{{/navdraweropen}}{{^navdraweropen}}false{{/navdraweropen}}" aria-controls="nav-drawer" type="button" class="btn nav-link float-sm-left mr-1" data-action="toggle-drawer" data-side="left" data-preference="drawer-open-nav"><i class="slicon-menu"></i><span class="sr-only">{{#str}}sidepanel, core{{/str}}</span></button>
        </div>
    {{/hasdrawertoggle}}

 
        <div class="container">
          <div class="logo">
            <img src="https://www.hst-et.com/images/logos/hst_logo.png" alt="Logo" data-aos="fade-right" data-aos-duration="3000" />
            <h3 class="big-title" data-aos="fade-up" data-aos-duration="3000">
              HST LEARNING SYSTEM
            </h3>
          </div>

         

          <div class="overlay"></div>

          <div class="hamburger-menu" data-aos="fade-down-right" data-aos-duration="3000">
            <div class="bar"></div>
          </div>
          </div>
      
     

   
    <ul class="nav navbar-nav ml-auto">
        <div class="d-none d-lg-block">
            {{{ output.search_box }}}
        </div>

        <li class="nav-item lang-menu">
            {{{ output.render_lang_menu }}}
        </li>

        {{{ output.navbar_plugin_output }}}

        {{{ output.user_menu }}}
    </ul>
</nav>
</div>
<header class="animate">